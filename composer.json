{
    "name": "sumanion/laravel-cloudflare",
    "description": "Simple way to determine real client IP and country when using CloudFlare",
    "keywords": ["laravel", "cloudflare", "ip", "country"],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ion Suman",
            "email": "sumanion122@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.3",
        "symfony/http-foundation": "^3.1|^4.0",
        "illuminate/http": "^5.3",
        "illuminate/container": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "SumanIon\\" : "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
